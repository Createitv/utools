{
  "name": "utools",
  "version": "1.0.0",
  "description": "utools",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Createitv/utools"
  },
  "type": "module",
  "scripts": {
    "universal:dev": "pnpm run -C packages/universal dev",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "eslint:comment": "eslint packages/**/*.ts",
    "eslint:fix": "eslint packages --ext .ts --max-warnings=0 --fix",
    "prettier:fix": "prettier --write \"packages/**/*.ts\"",
    "commit": "pnpm run eslint:fix && git add . && cz",
    "build": "npm run eslint && npm run prettier && rm -rf lib && tsc --build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@jest/globals": "^29.4.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.7",
    "eslint": "8.18.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rollup": "^3.15.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vitepress": "1.0.0-alpha.46",
    "vue": "^3.2.47"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "pnpm run prettier:fix"
    ]
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "private": true,
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-typescript2": "^0.34.1"
  }
}
